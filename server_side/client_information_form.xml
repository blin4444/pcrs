<?xml version="1.0"?>
<form>
    <div id="User_Info">
        <input id="first_name" name="First Name" type="text"/>
        <input id="middle_name" name="Middle Name" type="text" placeholder="n/a"/>
        <input id="last_name" name="Last Name" type="text"/>
        <br/>
        <input id="sin" name="SIN Number" type="number"/>
        <input id="date_of_birth" name="Birth Date" type="date"/>
        <input id="gender" name="Gender" type="gender" />
        <br/>
        <input id="street_address" name="Street Address" type="text"/>
        <input id="city" name="City" type="text"/>
        <input id="province" name="Province" type="text"/>
        <input id="postal_code" name="Postal Code" type="text"/>
        <br/>
        <input id="phone" name="Phone Number" type="tel" placeholder="n/a"/>
        <input id="alternate_phone" name="Alternate Phone Number" type="tel" placeholder="n/a"/>
        <input id="email" name="Email" type="text" placeholder="n/a"/>
		<br/>
		<br/>
        <input id="es_id" name="Employment Situation" type="list"/>
        <input id="ud_id" name="Unemployment Duration" type="list" placeholder="n/a"/>
        <br/>
        <input id="ga_id" name="Government Assistance" type="list" placeholder="n/a"/>
        <input id="rs_id" name="Residency Status" type="list"/>
		<br/>
		<br/>
        <input id="ou_id" name="Office Use" type="list"/>
    </div>
</form>
